a {
color: #000;
text-decoration: none;
}
a:hover,
a:focus {
color: #000;
text-decoration: underline;
}
.dropdown-menu {
font-size: 16px;
}
.breadcrumb > li + li::before {
padding: 0 5px;
color: #777;
content: ">> ";
}
.breadcrumb > .active {
color: <%= $ref_theme_color_list->{light} %>;
}
.header_style{
border-top-left-radius:4px;
font-weight: bold;
font-size:17px;
background-color:<%= $ref_theme_color_list->{light} %>;
padding: 8px 8px 6px 10px;
color:#000000;
text-decoration: none;
}
@media (max-width: 767px) {
  .header_style{
  border-top-left-radius:0px;
  }
}
#enter_button{
margin-top:3px;
background-color: <%= $ref_theme_color_list->{light} %>;
font-weight:bold;
border: 0   
}
#_exit_button{
margin-top:3px;
font-weight:bold;
border: 0   
}
#forgotn_button{
background-color:#ffffff;
color:<%= $ref_theme_color_list->{dark} %>;
padding-left:0px;
margin-top:6px;
border-width: 0px;    
}
.pagination > .active > span,
.pagination > .active > span:hover {
background-color:<%= $ref_theme_color_list->{light} %>;
border-color:<%= $ref_theme_color_list->{light} %>;
font-weight:bold;   
}
.pagination > li > a,
.pagination > .disabled,
.pagination > .disabled > a,
.pagination > .disabled > a:hover, 
.pagination > .disabled > a:focus {
color:<%= $ref_theme_color_list->{light} %>;
font-weight:bold;
border-color:silver;
}
.label_style{
color:<%= $ref_theme_color_list->{dark} %>;
}
.link_button_clear{
background-color:#ffffff;
font-weight:bold;
color:<%= $ref_theme_color_list->{dark} %>;
padding:0px;
margin-bottom:4px;
border-width:0px;   
}
.arch_link{
color:<%= $ref_theme_color_list->{dark} %>;
font-weight:bold;
}
.normal{
font-weight: normal;
}
.breadcr_href{
color:<%= $ref_theme_color_list->{dark} %>;    
}
.titl_auth {
color:<%= $ref_theme_color_list->{dark} %>;
padding-top: 64px;
font-weight:bold;
}
.footer{
background-color: #000;
color: #C9C9C9; 
margin-left:-15px; 
margin-right:-15px; 
margin-top:10px    
}
.footer a,
.footer a:visited,
.footer a:hover,
.footer a:active{
color: #C9C9C9;
text-decoration: none;
}
.footer-copyright{
background-color: <%= $ref_theme_color_list->{normal} %>;
color: #333;
padding: 6px;
margin-bottom: -4px;
}
/*------------------------------------------*/
.navbar-default .navbar-nav > li > a.icon{
display: block;
}
/*===========================================================================*/
.containr {
background-color: <%= $ref_theme_color_list->{light} %>;
}
.nav_horbar {
background-color: <%= $ref_theme_color_list->{light} %>;
}
.nav_horbar a.icon:hover {
background-color: <%= $ref_theme_color_list->{normal} %>;
}
.item-left {
background-color: <%= $ref_theme_color_list->{light} %>;
}
.menu {
background-color: <%= $ref_theme_color_list->{light} %>;
}
@media (max-width: 768px) {
  .menu a:first-child{
  border-top: 1px solid <%= $ref_theme_color_list->{normal} %>;
}
}
.activ{
background-color: <%= $ref_theme_color_list->{normal} %>;
}
.dropbtn:not(:hover){
background-color: <%= $ref_theme_color_list->{light} %>;
}
.nav_horbar a:hover, .drop_down:hover {
background-color: <%= $ref_theme_color_list->{normal} %>;
}
.hoverbg{
background-color: <%= $ref_theme_color_list->{normal} %> !important;
}
.nav_horbar .icon {
background-color: <%= $ref_theme_color_list->{light} %>;
}
#alternav{
background-color: <%= $ref_theme_color_list->{dark} %>;
}
/*===========================================================================*/
.more_button{
background-color: <%= $ref_theme_color_list->{light} %>; 
width:100%;
padding: 6px;
border-radius: 4px;
fint-weight: bold;
border:0px;
color: <%= $ref_theme_color_list->{dark} %>; 
}
.to-top{
cursor: pointer; 
text-align: center; 
position: fixed; 
right: 40px; 
background-color: <%= $ref_theme_color_list->{light} %>; 
opacity: 0.8; 
padding: 10px; width: 50px; 
height:50px; 
font-size: 24px; 
border-radius: 5px; 
color: #fff; 
display: none; 
z-index: 10
}
.to-top:hover{
background-color: <%= $ref_theme_color_list->{normal} %>;
}
.rectngl{
display: inline-block; 
position: relative; 
border: 1px solid <%= $ref_theme_color_list->{normal} %>; 
width: 54px; 
height: 10px; 
margin-left: 16px; 
margin-bottom: -4px;  
}